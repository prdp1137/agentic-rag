OPENAI_API_KEY=sk-your-openai-api-key-here
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=enterprise_rag

LLM_MODEL=gpt-5.1
EMBEDDING_MODEL=text-embedding-3-small

# Grading Strategy: parallel | batch | skip
# - parallel: Grade docs in parallel (fast, 1 LLM call per doc)
# - batch: Grade all docs in 1 LLM call (cheapest)
# - skip: No LLM grading, use retrieval score only (free)
GRADING_STRATEGY=parallel

SCORE_THRESHOLD_FOR_SKIP=0.01

RERANKER_TYPE=none
CROSS_ENCODER_MODEL=cross-encoder/ms-marco-MiniLM-L-6-v2
COHERE_API_KEY=your-cohere-api-key
COHERE_RERANK_MODEL=rerank-english-v3.0